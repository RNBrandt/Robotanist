        <script id="wikipedia-template" type="text/x-handlebars-template">
          <br/>
          <h3>{{title}}</h3>
          <hr>
          {{#each info}}
          {{{this.innerHTML}}}
          <br/>
          {{/each}}
        </script>


        <script id="flickr-template" type="text/x-handlebars-template">
          <h3>{{title}} Photos</h3>
          <hr>
          {{#each photos}}
          <div class="medium-3 large-3 columns text-center" style="margin-bottom: 20px;">
            <a class="th" href="../img/demos/demo2.png"><img src="https://farm{{this.farm}}.staticflickr.com/{{this.server}}/{{this.id}}_{{this.secret}}_m.jpg" style="height: 160px;"></a>
            <br>
            <h5><small>{{this.title}}</small></h5>
          </div>
          {{/each}}
        </script>
